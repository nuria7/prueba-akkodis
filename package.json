{
    "name": "Prueba tecnica",
    "version": "0.7.0",
    "scripts": {
        "ng": "ng",
        "start:local": "ng serve -c local",
        "start:local:nolive": "ng serve -c local --no-live-reload",
        "start:aot": "ng serve --open --aot",
        "start:prod": "ng serve --configuration production --open",
        "serve": "concurrently -c \"green,magenta\" \"npm:serve:api\" \"npm:serve:app\"",
        "serve:ie11": "concurrently -c \"green,magenta\" \"npm:serve:api\" \"npm:serve:app:ie11\"",
        "serve:api": "node ./api/api-server.js",
        "serve:app": "ng serve",
        "serve:app:ie11": "ng serve --configuration=es5",
        "build": "ng build",
        "build:prod": "ng build --configuration production --aot",
        "jest": "jest",
        "jest:coverage": "jest --coverage --maxWorkers=50% --silent",
        "jest:failures": "jest --onlyFailures --maxWorkers=50%",
        "jest:changed": "jest --onlyChanged --maxWorkers=50%",
        "jest:ci": "jest --runInBand",
        "jest:ci:coverage": "jest --runInBand --coverage --silent",
        "jest:watch": "jest --watch --maxWorkers=25%",
        "test:cc": "jest --coverage",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "pretty": "prettier --config .prettierrc.json --write \"src/{app,environments}/**/*{.ts,.js,.json,.html,.css,.scss}\"",
        "prepare": "husky install"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "14.0.6",
        "@angular/cdk": "^14.2.7",
        "@angular/cli": "^10.2.4",
        "@angular/common": "14.0.6",
        "@angular/compiler": "14.0.6",
        "@angular/core": "14.0.6",
        "@angular/flex-layout": "14.0.0-beta.41",
        "@angular/forms": "14.0.6",
        "@angular/language-service": "14.0.6",
        "@angular/localize": "14.0.6",
        "@angular/material": "14.2.7",
        "@angular/material-moment-adapter": "14.2.7",
        "@angular/platform-browser": "14.0.6",
        "@angular/platform-browser-dynamic": "14.0.6",
        "@angular/router": "14.0.6",
        "@fortawesome/fontawesome-free": "^6.4.2",
        "@kolkov/angular-editor": "^2.1.0",
        "@material/dialog": "14.0.0",
        "@ng-bootstrap/ng-bootstrap": "^15.1.1",
        "@ngx-matomo/router": "^3.2.1",
        "@ngx-matomo/tracker": "^3.2.1",
        "@ngx-translate/core": "^14.0.0",
        "@ngx-translate/http-loader": "7.0.0",
        "bootstrap": "^5.3.2",
        "chart.js": "3.9.1",
        "classlist.js": "1.1.20150312",
        "core-js": "3.23.4",
        "css-vars-ponyfill": "2.4.4",
        "eslint": "8.23.0",
        "moment": "^2.29.4",
        "ng2-charts": "4.0.1",
        "ngx-infinite-scroll": "^14.0.1",
        "ngx-pagination": "6.0.2",
        "rxjs": "7.5.0",
        "tslib": "2.3.0",
        "url-search-params-polyfill": "7.0.0",
        "zone.js": "0.11.4"
    },
    "devDependencies": {
        "@angular-builders/jest": "10.0.1",
        "@angular-devkit/build-angular": "^14.2.1",
        "@angular-eslint/schematics": "14.0.3",
        "@angular/compiler-cli": "14.0.6",
        "@ngneat/spectator": "11.1.0",
        "@types/jest": "27.5.2",
        "@types/node": "18.0.4",
        "copyfiles": "2.4.1",
        "husky": "7.0.0",
        "jest": "28.1.3",
        "jest-extended": "2.0.0",
        "jest-jasmine2": "27.5.1",
        "jest-preset-angular": "12.2.0",
        "jest-sonar": "0.2.12",
        "lint-staged": "12.3.7",
        "prettier": "2.5.0",
        "pretty-quick": "3.1.2",
        "ts-loader": "8.0.17",
        "ts-node": "10.8.0",
        "typescript": "4.7.4",
        "webcrypto-shim": "0.1.5",
        "whatwg-fetch": "3.0.0"
    }
}